<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Drag and drop Suite" verbose="-1" parallel="tests" thread-count="10">

    <test name="Moving blocks tests">
        <classes>
            <class name="DragAndDropTest">
                <methods>
                    <include name="moveBlockAtoBlockB"/>
                    <include name="moveBlockBtoBlockA"/>
                </methods>
            </class>
        </classes>
    </test>
    
    <test name="Getting page information">
        <classes>
            <class name="DragAndDropTest">
                <methods>
                    <include name="getPageTitle"/>
                </methods>
            </class>
        </classes>
    </test>
    
</suite>