<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="General suite" verbose="-1" parallel="tests" thread-count="10">

    <test name="Jquery tests">
        <classes>
            <class name="com.solvd.theinternet_solvd.jquery.JQueryTests"/>
        </classes>
    </test>

    <test name="File upload tests">
        <classes>
            <class name="com.solvd.theinternet_solvd.fileUpload.FileUploadTests"/>
        </classes>
    </test>

    <test name="Moving blocks tests">
        <classes>
            <class name="com.solvd.theinternet_solvd.drag_and_drop_test.DragAndDropTest">
                <methods>
                    <include name="moveBlockAtoBlockB"/>
                    <include name="moveBlockBtoBlockA"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Login Form Test">
        <classes>
            <class name="com.solvd.theinternet_solvd.loginFormTest.LoginFormTests"/>
        </classes>
    </test>

    <test name="Key Presses Tests">
        <classes>
            <class name="com.solvd.theinternet_solvd.keyPresses.KeyPressesTest">
                <methods>
                    <include name="successfulRandomTest"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>